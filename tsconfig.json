{
  "include": ["./**/*.ts"],
  "compilerOptions": {
    "jsx": "react",
    "target": "ES2020",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "docker/dist",
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  }
}
